<Window x:Class="Csharp_lab2.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Csharp_lab2"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <ToolBarTray HorizontalAlignment="Left" Height="94" Margin="10,10,0,0" VerticalAlignment="Top" Width="772">
            <ToolBar HorizontalAlignment="Left" Height="74" Margin="10,10,-112,-33" VerticalAlignment="Top" Width="202">
                <Menu HorizontalAlignment="Left" Height="31" VerticalAlignment="Top" Width="162" Margin="0,19,0,0">
                    <MenuItem Header="_File" Height="31" Margin="0" Width="100">
                        <MenuItem Header="_Open" Height="30" Margin="0" Click="menuClicked"/>
                        <MenuItem Header="_Test" Height="31" Margin="0"/>
                        <Separator/>
                        <MenuItem Header="_Exit" Height="30" Margin="0" Click="menuClicked"/>
                    </MenuItem>
                </Menu>
            </ToolBar>
        </ToolBarTray>
        <TreeView HorizontalAlignment="Left" Height="300" Margin="10,109,0,0" VerticalAlignment="Top" Width="772"/>

    </Grid>
</Window>
